{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "max-len": 1,
    "no-console": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "eol-last": 2,
    "camelcase": 0,
    "consistent-return": 0
  }
}
